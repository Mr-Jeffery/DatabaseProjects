// Subway System ER Diagram
digraph {
	L [label=Line shape=record]
	S [label=Station shape=record]
	P [label=Passenger shape=record]
	C [label=Card shape=record]
	R [label=Ride shape=record]
	LS [label=LineStation shape=record]
	LID [label="ID (PK)" shape=plaintext]
	LN [label=Name shape=plaintext]
	LST [label="Start Time" shape=plaintext]
	LET [label="End Time" shape=plaintext]
	LI [label=Intro shape=plaintext]
	LM [label=Mileage shape=plaintext]
	LC [label=Color shape=plaintext]
	LFO [label="First Opening" shape=plaintext]
	LU [label=URL shape=plaintext]
	SID [label="ID (PK)" shape=plaintext]
	SEN [label="English Name" shape=plaintext]
	SD [label=District shape=plaintext]
	SBI [label="Bus Info" shape=plaintext]
	SOI [label="Out Info" shape=plaintext]
	SI [label=Intro shape=plaintext]
	SCN [label="Chinese Name" shape=plaintext]
	PID [label="ID Number (PK)" shape=plaintext]
	PN [label=Name shape=plaintext]
	PP [label="Phone Number" shape=plaintext]
	PG [label=Gender shape=plaintext]
	PD [label=District shape=plaintext]
	CC [label="Code (PK)" shape=plaintext]
	CM [label=Money shape=plaintext]
	CT [label="Create Time" shape=plaintext]
	RID [label="Ride ID (PK)" shape=plaintext]
	RUS [label="User (FK)" shape=plaintext]
	RSS [label="Start Station (FK)" shape=plaintext]
	RES [label="End Station (FK)" shape=plaintext]
	RP [label=Price shape=plaintext]
	RST [label="Start Time" shape=plaintext]
	RET [label="End Time" shape=plaintext]
	LSLID [label="Line ID (FK)" shape=plaintext]
	LSSID [label="Station ID (FK)" shape=plaintext]
	LSO [label=Order shape=plaintext]
	L -> LID
	L -> LN
	L -> LST
	L -> LET
	L -> LI
	L -> LM
	L -> LC
	L -> LFO
	L -> LU
	S -> SID
	S -> SEN
	S -> SD
	S -> SBI
	S -> SOI
	S -> SI
	S -> SCN
	P -> PID
	P -> PN
	P -> PP
	P -> PG
	P -> PD
	C -> CC
	C -> CM
	C -> CT
	R -> RID
	R -> RUS
	R -> RSS
	R -> RES
	R -> RP
	R -> RST
	R -> RET
	LS -> LSLID
	LS -> LSSID
	LS -> LSO
	L -> LS [label=has]
	S -> LS [label="served by"]
	P -> R [label=takes]
	C -> R [label="used for"]
	S -> R [label="starts/ends at"]
}
